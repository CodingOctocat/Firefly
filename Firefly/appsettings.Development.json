{
  "CccfApi": {
    "BaseUrl": "https://www.cccf.com.cn:8088",
    "CertificateNumberHyperlink": "/certSearch/showCert/{0}",
    "ProductNameHyperlink": "/certSearch/enterCert/{0}",
    "Endpoints": {
      "Query": "/certSearch/search"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=cccf.db"
  },
  "Firefly": {
    "StrictMode": true,
    "CheckReportNumber": false
  },
  "Cccf": {
    "LocalMode": true
  },
  "CccfScraper": {
    "MinInterval": 3000
  },
  "ColumnMappings": {
    "Preset": {
      "Description": "适用于「福建消防技术服务信息平台 > 建筑消防设施检测报告 > 检查项目概况附表」",
      "FireSystemColumn": -1,
      "ProductNameColumn": 2,
      "CountColumn": 3,
      "ModelColumn": 4,
      "EnterpriseNameColumn": 4,
      "CertificateNumberColumn": 5,
      "ReportNumberColumn": 5,
      "StatusColumn": 6,
      "ManufactureDateColumn": 7,
      "RemarkColumn": 8,
      "RuleColumns": 8,
      "SkipRowRules": [
        {
          "IsEnabled": true,
          "TargetColumn": 2,
          "MatchText": "电梯",
          "MatchCase": false,
          "UseFuzzyMatching": true
        },
        {
          "IsEnabled": true,
          "TargetColumn": 2,
          "MatchText": "供电",
          "MatchCase": false,
          "UseFuzzyMatching": true
        },
        {
          "IsEnabled": true,
          "TargetColumn": 2,
          "MatchText": "市电",
          "MatchCase": false,
          "UseFuzzyMatching": true
        },
        {
          "IsEnabled": true,
          "TargetColumn": 2,
          "MatchText": "发电机",
          "MatchCase": false,
          "UseFuzzyMatching": true
        }
      ]
    }
  }
}
